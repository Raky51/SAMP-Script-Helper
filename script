    Public Class Fenêtre1
        
        'Fonction Form_Load
        Public Sub Form_Load(ByVal sender As Object, ByVal e As System.EventArgs)
            'Cette fonction se déclenche à l'ouverture de la fenêtre.
            '
            'This function is launched during opening.
        End Sub
        
        'Fonction button1_Click
        Public Sub button1_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            spawnprotection = Me.checkBox1.Checked
            dmarena = Me.checkBox2.Checked
            'Get The Proporités.
            'Stock him in variables
            If spawnprotection = "True" Then
                Me.textBox1.Text = "public OnPlayerSpawn(playerid)" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerHealth(playerid, 99999);" & System.Environment.NewLine & " SetTimerEx(" & Microsoft.VisualBasic.ChrW(34) & "AntiSKEnd" & Microsoft.VisualBasic.ChrW(34) & ", 10000, false, " & Microsoft.VisualBasic.ChrW(34) & "i" & Microsoft.VisualBasic.ChrW(34) & ", playerid);" & System.Environment.NewLine & " SendClientMessage(playerid,0xFFFF, " & Microsoft.VisualBasic.ChrW(34) & "You Have 10 Seconds Spawn Protection" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & "" & System.Environment.NewLine & "forward AntiSKEnd(playerid);" & System.Environment.NewLine & "public AntiSKEnd(playerid)" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerHealth(playerid, 100.0);" & System.Environment.NewLine & " SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "Spawn Protection expired!" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "}"
            Else
                If dmarena = "True" Then
                    Me.textBox1.Text = "#include <zcmd> //You must have ZCMD" & System.Environment.NewLine & "" & System.Environment.NewLine & "CMD:dm(playerid, params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & "new id;" & System.Environment.NewLine & "if(sscanf(params, " & Microsoft.VisualBasic.ChrW(34) & "d" & Microsoft.VisualBasic.ChrW(34) & ", id)) return SendClientMessage(playerid, 0x02FBFFFF, " & Microsoft.VisualBasic.ChrW(34) & "/dm [1-2]" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "if(id = 1)" & System.Environment.NewLine & "{" & System.Environment.NewLine & "SetPlayerPos(playerid,2265.4893,2085.7769,100.3735,77.2707);" & System.Environment.NewLine & "}" & System.Environment.NewLine & "else if(id == 2)" & System.Environment.NewLine & "{" & System.Environment.NewLine & "SetPlayerPos(playerid,2265.4893,2085.7769,100.3735,77.2707);" & System.Environment.NewLine & "}" & System.Environment.NewLine & "return 1;" & System.Environment.NewLine & "}"
                End If
            End If
            If spawnprotection = True AndAlso dmarena = True Then
                Me.textBox1.Text = "#include <zcmd>" & System.Environment.NewLine & "" & System.Environment.NewLine & "public OnPlayerSpawn(playerid)" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerHealth(playerid, 999999.0);" & System.Environment.NewLine & " SetTimerEx(" & Microsoft.VisualBasic.ChrW(34) & "AntiSKEnd" & Microsoft.VisualBasic.ChrW(34) & ", 10000, false, " & Microsoft.VisualBasic.ChrW(34) & "i" & Microsoft.VisualBasic.ChrW(34) & ", playerid);" & System.Environment.NewLine & " SendClientMessage(playerid,0xFFFF, " & Microsoft.VisualBasic.ChrW(34) & "You Have 10 Seconds Spawn Protection" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & "" & System.Environment.NewLine & "forward AntiSKEnd(playerid); //We'll use this to avoid future warnings" & System.Environment.NewLine & "public AntiSKEnd(playerid) //When the Timer Ends, This callback is called" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerHealth(playerid, 100.0);" & System.Environment.NewLine & " SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "Spawn Protection expired!" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & "CMD:dm(playerid, params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & "new id;" & System.Environment.NewLine & "if(sscanf(params, " & Microsoft.VisualBasic.ChrW(34) & "d" & Microsoft.VisualBasic.ChrW(34) & ", id)) return SendClientMessage(playerid, 0x02FBFFFF, " & Microsoft.VisualBasic.ChrW(34) & "/dm [1-2]" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "if(id = 1)" & System.Environment.NewLine & "{" & System.Environment.NewLine & "SetPlayerPos(playerid,2265.4893,2085.7769,100.3735,77.2707);" & System.Environment.NewLine & "}" & System.Environment.NewLine & "else if(id == 2)" & System.Environment.NewLine & "{" & System.Environment.NewLine & "SetPlayerPos(playerid,2265.4893,2085.7769,100.3735,77.2707);" & System.Environment.NewLine & "}" & System.Environment.NewLine & "return 1;" & System.Environment.NewLine & "}"
            End If
        End Sub
        
        'Fonction button2_Click
        Public Sub button2_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            commands_cmd = Me.textBox2.Text
            Me.textBox3.Text = "#include <zcmd>" & System.Environment.NewLine & "CMD:" & commands_cmd & "(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & " //Do the function here !" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "} "
        End Sub
        
        'Fonction button3_Click
        Public Sub button3_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            'We get the Checked Propority And Stock it in Variable
            help = Me.radioButton1.Checked
            heal = Me.radioButton2.Checked
            kill = Me.radioButton3.Checked
            armour = Me.radioButton4.Checked
            If help = True Then
                Me.textBox4.Text = "#include <zcmd>" & System.Environment.NewLine & "" & System.Environment.NewLine & "CMD:help(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "==========================Help Command By SAMP Script Helper==========================" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & " SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "======================================================================================" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "} "
            End If
            If heal = True Then
                Me.textBox4.Text = "#include <zcmd>" & System.Environment.NewLine & "" & System.Environment.NewLine & "CMD:heal(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerHealth(playerid, 100);" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "} "
            End If
            If kill = True Then
                Me.textBox4.Text = "#include <zcmd>" & System.Environment.NewLine & "" & System.Environment.NewLine & "CMD:kill(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerHealth(playerid, 0);" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "} "
            End If
            If armour = True Then
                Me.textBox4.Text = "#include <zcmd>" & System.Environment.NewLine & "" & System.Environment.NewLine & "CMD:armour(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & " SetPlayerArmour(playerid, 100);" & System.Environment.NewLine & " return 1;" & System.Environment.NewLine & "} "
            End If
            'Finished.
        End Sub
        
        'Fonction button4_Click
        Public Sub button4_Click(ByVal sender As Object, ByVal e As System.EventArgs)
            nuke_admin = Me.radioButton5.Checked
            heal_admin = Me.radioButton6.Checked
            armour_admin = Me.radioButton7.Checked
            givecash_admin = Me.radioButton8.Checked
            If nuke_admin = True Then
                Me.textBox5.Text = "CMD:nuke(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & "   new pname[24], targetid, string[128];" & System.Environment.NewLine & "   if(!IsPlayerAdmin(playerid)) return SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "SERVER: Unknown Command." & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   if(sscanf(params, " & Microsoft.VisualBasic.ChrW(34) & "u" & Microsoft.VisualBasic.ChrW(34) & ", targetid)) return SendClientMessage(playerid, red, " & Microsoft.VisualBasic.ChrW(34) & "USAGE: /nuke [playerid/name]" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, red, " & Microsoft.VisualBasic.ChrW(34) & "ERROR: That player isn't online!" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   new Float: x, Float: y, Float: z;" & System.Environment.NewLine & "   GetPlayerName(targetid, pname, 24);" & System.Environment.NewLine & "   GetPlayerPos(targetid, x, y, z);" & System.Environment.NewLine & "   CreateExplosion(x, y, z, 7, 10);" & System.Environment.NewLine & "   format(string, 128, " & Microsoft.VisualBasic.ChrW(34) & "%s (%d) has died from a mysterious explosion..." & Microsoft.VisualBasic.ChrW(34) & ", pname, targetid);" & System.Environment.NewLine & "   SetPlayerHealth(targetid,0);" & System.Environment.NewLine & "   SendClientMessageToAll(0xFF00FFFF, string);" & System.Environment.NewLine & "   SendClientMessage(targetid, 0xFF00FFFF, " & Microsoft.VisualBasic.ChrW(34) & "You have died from a mysterious explosion..." & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   format(string, 128, " & Microsoft.VisualBasic.ChrW(34) & "You have nuked %s (%d)" & Microsoft.VisualBasic.ChrW(34) & ", pname, targetid);" & System.Environment.NewLine & "   return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & ""
            End If
            If heal_admin = True Then
                Me.textBox5.Text = "CMD:heal(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & "   new pname[24], targetid, string[128];" & System.Environment.NewLine & "   if(!IsPlayerAdmin(playerid)) return SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "SERVER: Unknown Command." & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   if(sscanf(params, " & Microsoft.VisualBasic.ChrW(34) & "u" & Microsoft.VisualBasic.ChrW(34) & ", targetid)) return SendClientMessage(playerid, red, " & Microsoft.VisualBasic.ChrW(34) & "USAGE: /heal [playerid/name]" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, red, " & Microsoft.VisualBasic.ChrW(34) & "ERROR: That player isn't online!" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   GetPlayerName(targetid, pname, 24);" & System.Environment.NewLine & "   SetPlayerHealth(targetid,100);" & System.Environment.NewLine & "   SendClientMessage(targetid, 0xFF00FFFF, " & Microsoft.VisualBasic.ChrW(34) & "You have been healed" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   format(string, 128, " & Microsoft.VisualBasic.ChrW(34) & "You have healed %s (%d)" & Microsoft.VisualBasic.ChrW(34) & ", pname, targetid);" & System.Environment.NewLine & "   return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & ""
            End If
            If armour_admin = True Then
                Me.textBox5.Text = "CMD:armour(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & "   new pname[24], targetid, string[128];" & System.Environment.NewLine & "   if(!IsPlayerAdmin(playerid)) return SendClientMessage(playerid, -1, " & Microsoft.VisualBasic.ChrW(34) & "SERVER: Unknown Command." & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   if(sscanf(params, " & Microsoft.VisualBasic.ChrW(34) & "u" & Microsoft.VisualBasic.ChrW(34) & ", targetid)) return SendClientMessage(playerid, red, " & Microsoft.VisualBasic.ChrW(34) & "USAGE: /armour [playerid/name]" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, red, " & Microsoft.VisualBasic.ChrW(34) & "ERROR: That player isn't online!" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   GetPlayerName(targetid, pname, 24);" & System.Environment.NewLine & "   SetPlayerArmour(targetid,100);" & System.Environment.NewLine & "   SendClientMessage(targetid, 0xFF00FFFF, " & Microsoft.VisualBasic.ChrW(34) & "You have been given armour" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "   format(string, 128, " & Microsoft.VisualBasic.ChrW(34) & "You have gave %s (%d) armour" & Microsoft.VisualBasic.ChrW(34) & ", pname, targetid);" & System.Environment.NewLine & "   return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & ""
            End If
            If givecash_admin = True Then
                Me.textBox5.Text = "CMD:gc(playerid,params[])" & System.Environment.NewLine & "{" & System.Environment.NewLine & "  if(IsPlayerAdmin(playerid))" & System.Environment.NewLine & "  {" & System.Environment.NewLine & "  new id, money, name[MAX_PLAYER_NAME], aname[MAX_PLAYER_NAME], str[128];" & System.Environment.NewLine & "  if(sscanf(params, " & Microsoft.VisualBasic.ChrW(34) & "ui" & Microsoft.VisualBasic.ChrW(34) & ", id, money)) return SendClientMessage(playerid, 0xCC0000AA, " & Microsoft.VisualBasic.ChrW(34) & "USAGE: /givecash <id/partofname> <amount>" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "  GivePlayerMoney(id, money);" & System.Environment.NewLine & "  GetPlayerName(playerid, aname, sizeof(aname));" & System.Environment.NewLine & "  GetPlayerName(id, name, sizeof(name));" & System.Environment.NewLine & "  format(str,sizeof(str), " & Microsoft.VisualBasic.ChrW(34) & "Admin %s has given you $%d!" & Microsoft.VisualBasic.ChrW(34) & ", aname, money);" & System.Environment.NewLine & "  SendClientMessage(id, -1, str);" & System.Environment.NewLine & "  format(str,sizeof(str), " & Microsoft.VisualBasic.ChrW(34) & "You have given %s $%d!" & Microsoft.VisualBasic.ChrW(34) & ", name, money);" & System.Environment.NewLine & "  SendClientMessage(playerid, -1, str);" & System.Environment.NewLine & "  }" & System.Environment.NewLine & "  else return SendClientMessage(playerid, 0xCC0000AA, " & Microsoft.VisualBasic.ChrW(34) & "You aren't an admin!" & Microsoft.VisualBasic.ChrW(34) & ");" & System.Environment.NewLine & "  return 1;" & System.Environment.NewLine & "}" & System.Environment.NewLine & ""
            End If
        End Sub
    End Class
