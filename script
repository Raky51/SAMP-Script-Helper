           Get
               If _Fenêtre1.IsDisposed Then _Fenêtre1 = New Global.SAMP_Script_Helper.Fenêtre1()
               Return _Fenêtre1
           End Get
           Set(ByVal value As Global.SAMP_Script_Helper.Fenêtre1)
               _Fenêtre1 = value
           End Set
      End Property
  End Module
